{
  "ConnectionStrings": {
    "Default": "Data Source = Recipe.db"
  },
  "Serilog": {
    "Using": [],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning",
        "CRUDRecipeEF": "Debug",
      }
    },
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": ".\\Logs\\CRUDRecipeEF.log",
          "outputTemplate": "[{Timestamp:MM/dd HH:mm:ss.fff} {Level:u3}]: {Message}{NewLine:1}{Exception:1}"
        }
      }
    ]
  }
}